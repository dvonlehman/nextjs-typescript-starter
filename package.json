{
  "name": "steller-web-player",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "copy-static": "mkdir -p build/static && cp -r static build/",
    "dev": "rimraf build && npm run copy-static && concurrently \"tsc --watch\" \"delay 5 && (cd build; nodemon --config ../nodemon.json --inspect -r source-map-support/register server.js)\"",
    "build": "rimraf build && tsc && (cd build; next build) && npm run copy-static",
    "start": "(cd build; NODE_PATH=./ NODE_ENV=production node -r source-map-support/register server.js)",
    "postinstall": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/winston": "^2.4.4",
    "config": "^2.0.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "glamor": "^2.20.40",
    "glamorous": "^4.13.1",
    "lodash-es": "^4.17.11",
    "next": "^7.0.1",
    "nextjs-dynamic-routes": "^2.1.9",
    "node-fetch": "^2.2.0",
    "query-string": "^6.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "serve-favicon": "^2.5.0",
    "source-map-support": "^0.5.9",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/config": "0.0.34",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/lodash-es": "^4.17.1",
    "@types/next": "^7.0.0",
    "concurrently": "^4.0.1",
    "delay-cli": "^1.1.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-node": "^7.0.1",
    "tsc-watch": "^1.0.30",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  }
}
