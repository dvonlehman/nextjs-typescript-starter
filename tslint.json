{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "interface-name": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "double", "avoid-escape", "avoid-template"],
    "member-access": false
  },
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts", "build/**/*.*", "dist/**/*.*"]
  }
}